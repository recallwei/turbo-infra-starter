{
  "name": "@infra/admin",
  "description": "Universal admin template",
  "author": "Bruce Song <recall4056@gmail.com> (https://github.com/recallwei/)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:staging": "pnpm type:check && NODE_ENV=staging vite build --mode staging",
    "build:prod": "pnpm type:check && NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "type:check": "tsc --noEmit --composite false",
    "cspell:check": "cspell \"**\"",
    "eslint:check": "eslint \"**/*.{ts,tsx}\" --color",
    "eslint:fix": "pnpm eslint:check --fix",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:fix": "prettier --write --ignore-unknown ."
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.18.2",
    "@ant-design/happy-work-theme": "^1.0.0",
    "@ant-design/icons": "^5.2.6",
    "@infra/utils": "workspace:*",
    "@tanstack/react-query": "^5.17.15",
    "@tanstack/react-query-devtools": "^5.17.15",
    "ahooks": "^3.7.8",
    "antd": "^5.13.1",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "echarts": "^5.4.3",
    "i18next": "^23.7.16",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "use-immer": "^0.9.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.168",
    "@iconify/react": "^4.1.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.33",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-auto-import-ahooks": "^0.0.2",
    "unplugin-auto-import-antd": "^0.0.1",
    "unplugin-icons": "^0.18.2",
    "vite": "5.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-progress": "^0.0.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "private": true,
  "license": "MIT"
}
